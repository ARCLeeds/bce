# RStudio
echo "BCE: Installing RStudio..."
RS_V=0.98.1103

RSTUDIO_URL=http://download1.rstudio.org/rstudio-${RS_V}-amd64.deb && \
RSTUDIO_DEB=$(basename ${RSTUDIO_URL}) && \
curl -L -O ${RSTUDIO_URL} && \
gdebi -n ${RSTUDIO_DEB} && \
rm -f ${RSTUDIO_DEB} && \
( echo "  DONE" ; etckeeper commit "$msg" ) || echo "  FAIL"
